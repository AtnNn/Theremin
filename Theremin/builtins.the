
data Bool = True | False

data Nat = Z | S Nat

if a b c =
   case a of
        True -> b
        False -> c

lte x y =
  case x of
    Z -> True
    S xx ->
      case y of 
        Z -> False
        S yy -> lte xx yy

not b =
  case b of
    True -> False
    False -> True

add a b =
  case a of
    Z -> b
    S c -> S (add c b)

mul a b =
  case a of
    Z -> Z
    S c -> add b (mul c b)